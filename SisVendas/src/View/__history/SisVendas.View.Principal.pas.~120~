unit SisVendas.View.Principal;

interface

uses
  Winapi.Windows,
  Winapi.Messages,
  System.SysUtils,
  System.Variants,
  System.Classes,
  Vcl.Graphics,
  Vcl.Controls,
  Vcl.Forms,
  Vcl.Dialogs,
  Vcl.Buttons,
  Vcl.ComCtrls,
  Vcl.StdCtrls,
  System.Generics.Collections,
  Vcl.ExtCtrls,
  AnsiStrings;

type
  TForm_Principal = class(TForm)
  private
    { Private declarations }
    procedure AlterarDadosCliente(Alterar: Boolean);
    procedure ClearFocusLabels;
    procedure SetFocusLabels(Obj: TObject);
  public
    { Public declarations }
  end;

var
  Form_Principal: TForm_Principal;

implementation

{$R *.dfm}

end.
